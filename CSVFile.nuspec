<?xml version="1.0"?>
<package >
	<metadata>
		<id>CSVFile</id>
		<version>3.0.5</version>
		<title>CSVFile</title>
		<authors>Ted Spence</authors>
		<owners>Ted Spence</owners>
		<license type="file">docs/LICENSE</license>
		<projectUrl>https://github.com/tspence/csharp-csv-reader</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<summary>Tiny and fast CSV and TSV parsing library (40KB) with zero dependencies.  Compatible with most dot net versions.</summary>
		<description>Tiny and fast CSV and TSV parsing library (40KB) with zero dependencies.  Compatible with most dot net versions.</description>
		<releaseNotes>
			Improved compatibility with multiple versions of the Dot Net framework.
			* Version 3.0.5 builds for Dot Net 4.0, 4.5, Net Standard 2.0, Net Core 3.1, 5.0, and 6.0
			* Some improvements on compiler warnings
			* Included XML documentation with modern Dot Net Core versions 
		</releaseNotes>
		<readme>docs/README.md</readme>
		<copyright>Copyright 2006 - 2022</copyright>
    	<tags>fast csv parser serialization deserialization streaming async</tags>
		<repository type="git" url="https://github.com/tspence/csharp-csv-reader" />
		<dependencies>
			<group targetFramework=".NETFramework4.0"/>
			<group targetFramework=".NETFramework4.5"/>
			<group targetFramework=".NETStandard2.0"/>
			<group targetFramework="netcoreapp3.1"/>
			<group targetFramework="net5.0"/>
			<group targetFramework="net6.0"/>
		</dependencies>
	</metadata>
	<files>
		<file src=".\LICENSE" target="docs/LICENSE"/>
		<file src=".\README.md" target="docs/README.md"/>
		<file src="src\net40\bin\debug\net40\*" target="lib\net40" />
		<file src="src\net45\bin\debug\net45\*" target="lib\net45" />
		<file src="src\net50\bin\debug\net5.0\*" target="lib\net5.0" />
		<file src="src\net50\bin\debug\net6.0\*" target="lib\net6.0" />
		<file src="src\net50\bin\Debug\netcoreapp3.1\*" target="lib\netcoreapp3.1" />
		<file src="src\netstandard20\bin\debug\netstandard2.0\*" target="lib\netstandard20" />
	</files>
</package>
